<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
        
        <hibernate-mapping>
                      <class name="com.har.entity.BankAccount" table="bank_account">
                       <id name="accountId" column="account_Id" type="java.lang.Integer">
                            <generator class="increment"/>
                      </id>
                         <property name="accountType" column="account_type" />
                         <property name="accountNumber" column="account_number" />
                          <property name="balance" />
                    </class>
        </hibernate-mapping>